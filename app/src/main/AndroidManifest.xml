<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.patientapp3">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SymptomsChecker_Activity"></activity>
        <activity android:name=".ImageFullScreen_Activity" />
        <activity
            android:name=".Prescription_Activity"
            android:label="Prescription"
            android:parentActivityName=".UserDashBoard_Activity"
            android:theme="@style/HeaderTheme" />
        <activity
            android:name=".userProfile_Activity"
            android:label="Profile"
            android:parentActivityName=".UserDashBoard_Activity"
            android:theme="@style/HeaderTheme" />
        <activity android:name=".UserDashBoard_Activity" />
        <activity
            android:name=".SignUp_Activity"
            android:label="SignUp"
            android:parentActivityName=".Login"
            android:theme="@style/HeaderTheme" />
        <activity
            android:name=".HospitalList_Activity"
            android:label="Hospital List"
            android:parentActivityName=".Hospital"
            android:theme="@style/HeaderTheme" />
        <activity
            android:name=".Login"
            android:label="LOGIN"
            android:parentActivityName=".MainActivity"
            android:theme="@style/HeaderTheme" />
        <activity android:name=".Chart" />
        <activity
            android:name=".Bloodbank"
            android:label="Blood Bank"
            android:theme="@style/HeaderTheme" />
        <activity
            android:name=".Healthtips"
            android:label="Health Tips"
            android:theme="@style/HeaderTheme" />
        <activity
            android:name=".Medicine"
            android:label="Medicine List"
            android:theme="@style/HeaderTheme" />
        <activity
            android:name=".Doctor"
            android:label="Doctor List"
            android:theme="@style/HeaderTheme" />
        <activity
            android:name=".Hospital"
            android:label="Find Hospital"
            android:theme="@style/HeaderTheme" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.theartofdev.edmodo.cropper.CropImageActivity"
            android:screenOrientation="fullSensor"
            android:theme="@style/HeaderTheme" />
    </application>

</manifest>